# Production Environment Terraform Variables
# Copy this file to terraform.tfvars and fill in your values
# terraform.tfvars should be added to .gitignore to protect sensitive data

project_id          = "your-gcp-project-id"
region              = "us-central1"
zone                = "us-central1-a"
user_email          = "your-email@example.com"
billing_account_id  = "your-billing-account-id"

# Workload Identity (recommended for production)
enable_workload_identity = false  # Set to true if using GKE with Workload Identity
